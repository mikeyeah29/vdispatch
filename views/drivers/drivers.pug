extends ../layout

block content
	include ../partials/_header

	section#page-accounts
		.container
			h1 Drivers
			ol.breadcrumb
				li.breadcrumb-item
					a(href="/drivers/overview") Drivers Overview
				li.breadcrumb-item.active View All Drivers
			ul.actions_bar
				li
					.search_group
						input(type="text", id="q_search")
						.btn.btn-search Search
				li.right
					a(href="/drivers/create-driver").btn.btn-default Add New Driver